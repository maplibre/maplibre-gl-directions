{
  "name": "@maplibre/maplibre-gl-directions",
  "version": "0.1.4",
  "license": "MIT",
  "homepage": "https://maplibre.org/maplibre-gl-directions/#/",
  "repository": "https://github.com/maplibre/maplibre-gl-directions",
  "keywords": [
    "directions",
    "osrm",
    "routing",
    "mapbox",
    "maplibre"
  ],
  "files": [
    "dist"
  ],
  "module": "./dist/maplibre-gl-directions.es.js",
  "types": "./dist/main.d.ts",
  "type": "module",
  "scripts": {
    "env:prep": "npm run build:lib && npm link && npm link @maplibre/maplibre-gl-directions",
    "dev:lib": "svelte-check --tsconfig ./tsconfig.lib.json && tsc --project ./tsconfig.lib.json && vite build --watch --mode development --config vite.lib.config.ts",
    "dev:doc": "typedoc --tsconfig tsconfig.lib.json --watch",
    "dev:demo": "vue-tsc && vite --config vite.demo.config.ts",
    "build": "npm run format && npm run build:lib && npm run build:doc && npm run build:demo",
    "format": "eslint --fix && prettier --write ./",
    "build:lib": "svelte-check --tsconfig ./tsconfig.lib.json && tsc --project ./tsconfig.lib.json && vite build --config vite.lib.config.ts",
    "build:doc": "typedoc --tsconfig tsconfig.lib.json",
    "build:demo": "vue-tsc && vite build --config vite.demo.config.ts --base /maplibre-gl-directions/"
  },
  "dependencies": {
    "@mapbox/polyline": "^1.1.1",
    "nanoid": "^3.3.3",
    "vue": "^3.2.33"
  },
  "peerDependencies": {
    "maplibre-gl": "^2.1.0"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^1.0.0-next.30",
    "@tailwindcss/forms": "^0.5.0",
    "@tsconfig/svelte": "^2.0.1",
    "@types/geojson": "^7946.0.8",
    "@types/mapbox__point-geometry": "^0.1.2",
    "@types/mapbox__polyline": "^1.0.2",
    "@types/mapbox__vector-tile": "^1.3.0",
    "@types/node": "^17.0.25",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "@vitejs/plugin-vue": "^2.3.1",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "autoprefixer": "^10.4.4",
    "eslint": "^8.13.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-svelte3": "^3.4.1",
    "eslint-plugin-vue": "^8.7.0",
    "maplibre-gl": "^2.1.9",
    "postcss": "^8.4.12",
    "prettier": "^2.6.2",
    "prettier-plugin-svelte": "^2.7.0",
    "rollup-plugin-visualizer": "^5.6.0",
    "svelte": "^3.44.0",
    "svelte-check": "^2.2.7",
    "svelte-preprocess": "^4.9.8",
    "tailwindcss": "^3.0.24",
    "tslib": "^2.3.1",
    "typedoc": "^0.22.15",
    "typescript": "^4.5.4",
    "vite": "^2.9.5",
    "vue-eslint-parser": "^8.3.0",
    "vue-router": "^4.0.14",
    "vue-tsc": "^0.29.8"
  }
}
